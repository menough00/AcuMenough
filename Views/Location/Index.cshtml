@model List<Location>

<h2>Locations</h2>

<table>
    <thead>
        <tr>
            <th>Location Name</th>
            <th>Location ID</th>
            <th>Is Clearance</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var location in Model)
        {
            <tr>
                <td>@location.LocationName</td>
                <td>@location.LocationId</td>
                <td>@location.IsClearance</td>
            </tr>
        }
    </tbody>
</table>